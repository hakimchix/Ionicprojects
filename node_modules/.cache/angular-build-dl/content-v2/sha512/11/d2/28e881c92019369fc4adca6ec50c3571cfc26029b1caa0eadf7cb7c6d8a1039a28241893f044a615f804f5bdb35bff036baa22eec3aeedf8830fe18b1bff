let TaskService = class TaskService {
    constructor(afAuth, afs) {
        this.afAuth = afAuth;
        this.tasksCollection = afs.collection('tasks');
        this.tasks = this.tasksCollection.snapshotChanges().pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_3__["map"])(actions => {
            return actions.map(a => {
                const data = a.payload.doc.data();
                const id = a.payload.doc.id;
                return Object.assign({ id }, data);
            });
        }));
    }
    getTasks() {
        return this.tasks;
    }
    getopenTasks(d) {
        return this.tasksCollection.snapshotChanges().pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_3__["map"])(actions => {
            return actions.map(a => {
                const data = a.payload.doc.data();
                const id = a.payload.doc.id;
                return Object.assign({ id }, data);
            }).filter(a => a.closed === d);
        }));
    }
    getTask(id) {
        return this.tasksCollection.doc(id).valueChanges();
    }
    closeTask(task, id) {
        task.closed = true;
        return this.tasksCollection.doc(id).update(task);
    }
    openTask(task, id) {
        task.closed = false;
        return this.tasksCollection.doc(id).update(task);
    }
    updateTask(task, id) {
        return this.tasksCollection.doc(id).update(task);
    }
    /*addTaskToFirebase() {
      this.db.list('Tasks/').push({
        text: 'this.myTask',
        date: new Date().toISOString(),
        checked: false
      });
      this.showForm();
    }*/
    addTask(task) {
        return this.tasksCollection.add(task);
    }
    removeTask(id) {
        return this.tasksCollection.doc(id).delete();
    }
};
